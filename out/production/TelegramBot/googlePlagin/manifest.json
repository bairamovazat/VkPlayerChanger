{
	"manifest_version": 2,

	"name": "Управление плеером Vk",
	"version": "1.0",

	"icons": {
		"16": "icon/16x16.png",
		"32": "icon/32x32.png",
		"48": "icon/128x128.png",
		"128": "icon/128x128.png"
	},

	"permissions": [],

	"browser_action": {
 	"default_title": "Управление плеером Vk",
 	"default_icon": "icon/128x128.png"
	},

	"background": {
    	"persistent": true,
    	"page": "background.html"
  	},
  	"web_accessible_resources":["js/change_vk_js.js"],
	"content_scripts": [
    {
      "matches": [
        "http://vk.com/*",
        "https://vk.com/*"
       ],
      "js": [ "js/vk.js" ]
    }
  ]


}